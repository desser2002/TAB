{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2018",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "types": ["express-session", "express"],
    "skipLibCheck": true,
    "sourceMap": true
  },
  "include": [
    "src/**/*.ts",
    "types/**/*.d.ts"
  ],
  "exclude": ["node_modules"]
}
